<div class="discover-page">
  <% if @quote %>
    <h1>Quote for <%= @discover_date.strftime('%B %d, %Y') %></h1>
    <p>Debug: User signed in = <%= user_signed_in? %></p>
    <p>Debug: Quote ID = <%= @quote.id %></p>
    <p>Debug: Quote text exists = <%= @quote.text.present? %></p>
  <% else %>
    <p>No quote found</p>
  <% end %>
</div>